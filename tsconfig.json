{
  "compilerOptions": {
    "target": "es2018",
    "module": "commonjs",
    "outDir": "functions-build",
    "rootDir": "functions",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["functions/**/*"],
  "exclude": ["node_modules"]
} 